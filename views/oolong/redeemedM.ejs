
<table class="table is-bordered is-striped is-hoverable is-fullwidth" id='table'>

<table>
<script>
 async function accessDB_()
 {
    var ID='<%= couponsID %>' 
    //console.log(ID);
    var response = await fetch('/oolong/'+ ID +'/clients', {
              method: "GET"
          });

    var dynamicCode='';
    if (response.ok) 
    {  
        var JsonData = await response.json();
        //console.log(JsonData);
        for(i=0;i<JsonData.clients.length;i++)
        {
            dynamicCode+='<tr><td>'+ JsonData.clients[i].username+'</td></tr>';
        }
        document.getElementById("table").innerHTML = dynamicCode ;
        
    }
 }  

 accessDB_();
</script>